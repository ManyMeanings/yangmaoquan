.wxSearch{
  position: absolute;
  top: 43px;
  left: 0;
  width: 100%;
  height: 100%;
  border-top: 1px #eee solid;
  background-color:white;
}
.wxSearchInner{
  background-color: #fff;
  height: fit-content;
}

.wxSearch-section{
  background-color:#fc7600;
  border: 1px #fc7600 solid;
  padding: 5rpx;
  width: 100%;
  height: 70rpx;
  border: 1rpx solid #fc7600;
}
.wxSearch-pancel{
  display: flex;
  flex-direction: row;
  background-color: #fc7600;
  border-radius: 20rpx;
  border: 1px  #fc7600 solid;
}

.wxSearch-input{
  flex: 8; 
  margin-top: 13rpx;
  font-weight: 100;
  font-size: 24rpx;
  background-color: #e26902;
  border-radius: 16rpx;
}
.wxSearch-button{
  border: none !important;
  color: white !important;
  font-size: 25rpx !important;
  margin-top: auto;
  font-weight: 100;
}

.wxSearchTitle{
  width: 124rpx;
  height: 46rpx;
  padding: 8rpx 5rpx 0rpx 10rpx;
  display: flex;
  font-size: 14px;
  font-style: unset;
  text-decoration: rgb(15, 48, 109);
  border: 1px solid #eee;
  margin: 16rpx 20rpx;
  border-radius: 30rpx;
  float: left;
  background-color: #f8f8f8;
  color: #a9a9a9;
}

.wxSearchMindKeyItem{
  padding: 10px 5px;
  border-bottom: 1px solid #eee;
  display: flex;
  font-size: 14px;
  color: rgb(15, 48, 109);
}

.wxSearchKeyList{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.wxSearchKeyItem{
  flex: 0 0 20%;
  font-size: 14px;
  text-align: center;
  border: 1px solid #eee;
  margin: 5px;
  padding: 5px 10px;
  border-radius: 5px;
}

.wxSearchHistoryItem{
  border: 1px solid #eee;
  height: fit-content;
  display: flex;
  background-color: #fff;
}
.wxSearchHistoryItemText{
  width: auto;
  height: 46rpx;
  padding: 10rpx 10rpx 10rpx 10rpx;
  font-size: 14px;
  font-style: unset;
  text-decoration: rgb(15, 48, 109);
  border: 1px solid #eee;
  border-radius: 30rpx;
  margin: 16rpx 20rpx; 
  float: left;
  background-color: #f8f8f8;
  color: #a9a9a9;
}

.wxSearchHistoryEmpty{
  text-align: center;
  height: 50px;
  line-height: 50px;
  font-size: 12px;
}
.wxSearchDeleteAllBtn{
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 12px;
}